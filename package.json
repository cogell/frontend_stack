    "gulp-eslint": "^0.4.2",
    "gulp-template": "^3.0.0",
    "gulp-util": "^3.0.3",
    "jest-cli": "^0.4.0",
    "jsx-loader": "^0.12.2",
    "lodash.assign": "^3.0.0",
